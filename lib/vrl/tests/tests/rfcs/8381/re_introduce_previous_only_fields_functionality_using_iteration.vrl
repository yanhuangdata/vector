# object: { "some": true, "other": false, "fields": "valid", "this one": "invalid" }
# result: { "some": true,                 "fields": "valid"                        }

only_fields = ["some", "set", "of", "fields"]
for_each(.) -> |key, _| {
  if !includes(only_fields, key) {
    . = remove!(., [key])
  }
}

.
