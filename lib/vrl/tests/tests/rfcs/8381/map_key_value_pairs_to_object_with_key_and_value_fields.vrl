# object: { "labels": { "key1": "value1", "key2": "value2" } }
# result: { "labels": [{ "key": "key1", "value": "value1" }, { "key": "key2", "value": "value2" }] }

new_labels = []
for_each(object!(.labels)) -> |key, value| {
  new_labels = push(new_labels, { "key": key, "value": value })
}

.labels = new_labels
.
